# Uncomment the next line to define a global platform for your project 取消注释下一行以定义项目的全局平台的最低版本
# platform :ios, '9.0'

target 'WB' do
  # Comment the next line if you don't want to use dynamic frameworks 如果不想使用动态框架，请注释下一行 ->Swift 中使用pods 需要采用frameworks 来使用第三方框架, 默认打开.
  use_frameworks!
  
  pod 'SnapKit'        # 跟Masonry一样是用来设置约束的，swift版
  pod 'Alamofire'      # 用于网络请求
  pod 'Kingfisher'     # SDWebImage swift 版
  pod 'ObjectMapper'   # 字典转模型
  pod 'pop'            # facebook 的动画库
  #pod 'RxSwift'
  #pod 'RxCocoa'
  #pod 'RxDataSources'   # RxSwift中用于设置UITableView/UICollectionView data sources
  # Pods for WB

end
